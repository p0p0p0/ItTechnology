<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
              android:background="@color/black"
    android:orientation="vertical"
    tools:context="com.yuntongxun.ecdemo.com.safetyhuge.ecdemo.ui.account.NewLauncherActivity">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输入自己登陆id"
        android:id="@+id/et_self_id"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输入联系人id"
        android:id="@+id/et_contact_id"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登陆"
        android:id="@+id/btnLogin" />
    <Button
        android:id="@+id/huihua"
        android:text="会话列表"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/huihua1"
        android:text="对话"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
